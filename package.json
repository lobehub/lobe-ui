    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@ant-design/icons": "^6.1.0",
    "@commitlint/cli": "^19.8.1",
    "@lobehub/lint": "^1.26.2",
    "@testing-library/react": "^16.3.0",
    "@types/chroma-js": "^3.1.2",
    "@types/hast": "^3.0.4",
    "@types/katex": "^0.16.7",
